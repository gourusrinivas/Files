pipeline {
    agent any
    stages {
        stage('single run') {
            parallel {
                stage('Parallel Test 1') {
                    agent 1 { label 'Slave' }
                    steps {
                        echo 'hello'
                    }
                }
                 stage('Parallel Test 2') {
                     agent 2 { label 'Slave' }
                     steps {
                         echo 'World'
                    }
                }
            }
        }
    }
 } 
