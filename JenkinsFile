pipeline {
    agent('Slave1') { label 'Slave' }
    stages { 
        stage('Python') { 
            steps {
                echo 'helloworld'
            }
        }
    }
    
    agent('Slave2') { label 'Slave' }
    stages { 
        stage('Python2') { 
            steps {
                echo 'hello'
            }
        }
    }
    
 }
            
