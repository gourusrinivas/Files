pipeline {
    stages {
            parallel {
                stage('Parallel Test 1') {
                    agent { label 'Slave' }
                    steps {
                        echo 'hello'
                    }
                }
    
                 stage('Parallel Test 2') {
                     agent { label 'Slave' }
                     steps {
                         echo 'World'
                    }
                }
            }
        }
    }
}
           
