pipeline {
    agent none
    stages { 
        stage('Python') { 
            agent { label 'Slave' }
            steps {
                echo 'helloworld'
            }
        }
        stage('Python1') { 
            agent("Slave2") { label 'Slave' }
            steps {
                echo 'hello'
            }
        } 
     }
 }
            
