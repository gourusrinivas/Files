pipeline {
    agent any
    stages { 
        stage('Python') { 
            agent('Slave1'){
                label 'Slave'
             }
            steps {
                echo 'helloworld'
            }
        }
        
        stage('Python1') {
            agent('Slave2'){
                label 'Slave'
            }
           steps {
               echo 'Hello'
           }
       }
    }
 }
            
