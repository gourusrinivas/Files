pipeline {
    agent none
    stages {
             stage('Parallel Test 1') {
                agent  { label 'Slave' }
                    steps { 
                      
                       build(job : 'S_with_json.py')
                    }
                }
             stage('Parallel Test 2') {
                agent  { label 'Slave' }
                     steps {
                        
                        build(job : 'S_with_json.py')
                  }
             }
         }
  }
  
