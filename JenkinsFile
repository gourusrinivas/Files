pipeline {
    agent none
    stages {
        stage('single run') {
            parallel {
                stage('Parallel Test 1') {
                    agent  { label 'Slave' }
                    steps {
                        bat 'S_with_json.py'
                    }
                }
                 stage('Parallel Test 2') {
                     agent  { label 'Slave' }
                     steps {
                        bat 'S_with_json.py'
                    }
                }
            }
        }
    }
 } 
