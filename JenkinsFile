pipeline {
    agent none
    stages {
             stage('Parallel Test 1') {
                agent  { label 'Slave' }
                    steps { 
                       parallel p1: { 
                           build 'p1' } ,
                       p2 : { 
                           build 'p2' }
                    }
                }
             stage('Parallel Test 2') {
                agent  { label 'Slave' }
                     steps {
                        build 'p3'
                  }
             }
         }
  }
  
