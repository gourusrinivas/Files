pipeline {
   agent any
    stages {
        stage('single run') {
            parallel {
                stage('Parallel Test 1') {
                    steps {
                        agent { label 'Slave' }
                        echo 'hello'
                    }
                }
    
                 stage('Parallel Test 2') {
                     steps {
                         agent { label 'Slave' }
                         echo 'World'
                    }
                }
            }
        }
    }
}
           
