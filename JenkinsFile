pipeline {
    agent none
    stages {
             stage('Parallel Test 1') {
                agent  { label 'Slave' }
                    steps {
                        bat 'S_with_json.py'
                    }
                }
             stage('Parallel Test 2') {
                agent  { label 'Slave' }
                     steps {
                        echo 'HEllo world'
                  }
             }
         }
   }
   
